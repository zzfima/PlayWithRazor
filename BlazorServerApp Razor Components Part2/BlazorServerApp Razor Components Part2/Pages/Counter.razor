@page "/counter"

<PageTitle>Counter</PageTitle>

<h1>Counter</h1>

<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>
<br />
<ContactComponent CurrentContact="@(new Contact{FirstName="Zaza", LastName= "Abubaba", Email="avx@gmail.com"})" />
<br />
<TextBoxComponent placeholder="Is a Sample placeholder" type="text" disabled="disabled"/>

@code {
	private int currentCount = 0;

	private void IncrementCount()
	{
		currentCount++;
	}
}
